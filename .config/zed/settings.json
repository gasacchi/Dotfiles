{
  "auto_update": false,
  "buffer_font_family": "MonaspiceXe Nerd Font",
  "buffer_font_features": {
    "calt": true
  },
  "buffer_font_size": 14,
  "confirm_quit": true,
  "scrollbar": {
    "show": "never"
  },
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "tabs": {
    "git_status": true
  },
  "wrap_guides": [80],
  "relative_line_numbers": true,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "font_family": "MonaspiceXe Nerd Font"
  },
  "ui_font_size": 16,
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "Catppuccin Mocha"
  },
  "vim_mode": true,
  "languages": {
    "C": {
      "hard_tabs": true
    }
  }
}
